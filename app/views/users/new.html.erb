<h1>Signup</h1>
<%= form_for @user do |f| %>
  <%= render 'error' %>
  <div>
    f.label :email
    f.text_field :email
  </div>
  <div>
    f.label :email
    f.text_field :email
  </div>
  <div>
    f.label :password
    f.password_field :password
  </div>
  <div>
    f.label :password_confirmation
    f.text_password :password_confirmation
  </div>
<% end %>
